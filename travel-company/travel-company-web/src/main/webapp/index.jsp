        function getAndParseJson(url, onSuccess, onError) {
            get(
                url,
                function (response) {
                    var data;
                    try {
                        data = JSON.parse(response.responseText);
                    } catch (err) {
                        console.error('Error occurred when parsing response of:', url, err);
                        return onError(err);
                    }

                    onSuccess(data);
                },
                onError
            );
        }

        function get(url, onSuccess, onError) {
            var xhr = request('GET', url, onSuccess, onError);
            xhr.send();
        }

        function request(method, url, onSuccess, onError) {
            var req = new XMLHttpRequest();
            req.open(method, url);

            req.onreadystatechange = function () {
                // you cannot read any property on req until the readyState is 4
                if (req.readyState !== XMLHttpRequest.DONE) return;
                if ((req.status >= 500 || req.status === 0) && onError) return onError(req);
                if (req.status === 200 && onSuccess) onSuccess(req);
            };

            return req;
        }

        function loadJs(url) {
            var element = document.createElement('script');
            element.async = true;
            element.src = url;
            // inject into document to kick off loading
            document.body.appendChild(element);
        }

        function loadCss(url) {
            var element = document.createElement('link');
            element.type = 'text/css';
            element.rel = 'stylesheet';
            element.href = url;
            // inject into document to kick off loading
            document.head.appendChild(element);
        }
